{
  "name": "fostam/simplerouter",
  "type": "library",
  "description": "Simple router, mainly for API backends, with CORS support",
  "keywords": ["router", "api", "rest", "cors"],
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5|^6.5"
  },
  "authors": [
    {
      "name": "Stefan Stammler",
      "email": "stefan@fostam.org"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Fostam\\SimpleRouter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fostam\\SimpleRouter\\Tests\\": "tests"
    }
  }
}
